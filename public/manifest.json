{
  "manifest_version": 3,
  "name": "Float AI",
  "version": "1.0.0",
  "description": "An AI-powered tool that enhances your browsing experience.",
  "action": {
    "default_title": "Float AI",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["storage", "activeTab", "sidePanel", "runtime"],
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
