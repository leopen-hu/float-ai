{
  "name": "float-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm build:main && pnpm build:content",
    "gen-route": "vite build",
    "build:main": "tsc -b && vite build && pnpm move-index",
    "build:content": "vite build -c vite.content.config.ts",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "move-index": "mv dist/src/sidebar/index.html dist/sidebar/index.html && rm -rf dist/src"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/vite": "^4.0.12",
    "@tanstack/react-router": "^1.114.25",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^24.2.2",
    "idb": "^8.0.2",
    "lucide-react": "^0.479.0",
    "next-themes": "^0.4.5",
    "openai": "^4.86.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-markdown": "^10.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
